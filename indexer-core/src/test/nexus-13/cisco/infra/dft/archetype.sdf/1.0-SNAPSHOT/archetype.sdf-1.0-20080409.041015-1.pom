<?xml version="1.0" encoding="UTF-8"?>
<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>cisco.infra.dft</groupId>
    <artifactId>archetype.sdf</artifactId>
    <packaging>maven-archetype</packaging>
    <name>SDF Archetype Plugin</name>
    <version>1.0-SNAPSHOT</version>
    <parent>
        <groupId>cisco.infra.dft</groupId>
        <artifactId>parent.pom</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>

    <scm>
        <connection>scm:svn:http://dft-releng-03.cisco.com/svn/cisco.repo/cisco.infra/dft/archetype.sdf/trunk/
        </connection>
        <developerConnection>
            scm:svn:http://dft-releng-03.cisco.com/svn/cisco.repo/cisco.infra/dft/archetype.sdf/trunk/
        </developerConnection>
        <url>http://dft-releng-03.cisco.com/svn/cisco.repo/cisco.infra/dft/archetype.sdf/trunk/</url>
    </scm>

    <!-- ============================================================= -->
    <!-- DEVELOPERS                                                    -->
    <!-- ============================================================= -->
    <developers>
        <developer>
            <id>robevans</id>
            <name>Rob Evans</name>
            <email>robevans@cisco.com</email>
            <url>http://wwwin-tools.cisco.com/dir/details/robevans</url>
            <organization>CA SIS</organization>
            <organizationUrl>
                http://wwwin-tools.cisco.com/dir/details/robevans
            </organizationUrl>
            <roles>
                <role>architect</role>
                <role>developer</role>
                <role>lead</role>
            </roles>
            <timezone>-3</timezone>
            <properties>
                <picUrl>
                    http://wwwin.cisco.com/dir/photo/zoom/robevans.jpg
                </picUrl>
            </properties>
        </developer>

        <developer>
            <id>dkellam</id>
            <name>Dave Kellam</name>
            <email>dkellam@cisco.com</email>
            <url>http://wwwin-tools.cisco.com/dir/details/robevans</url>
            <organization>CA SIS</organization>
            <roles>
                <role>developer</role>
            </roles>

        </developer>

    </developers>


    <ciManagement>
        <system>continuum</system>
        <notifiers>
            <notifier>
                <type>mail</type>
                <sendOnError>true</sendOnError>
                <sendOnFailure>true</sendOnFailure>
                <sendOnSuccess>true</sendOnSuccess>
                <sendOnWarning>true</sendOnWarning>
                <configuration>
                    <address>anobansa@cisco.com</address>
                </configuration>
            </notifier>
        </notifiers>
    </ciManagement>

    <build>
        <extensions>
            <extension>
                <groupId>org.apache.maven.archetype</groupId>
                <artifactId>archetype-packaging</artifactId>
                <version>2.0-alpha-3-SNAPSHOT</version>
            </extension>
        </extensions>
        <plugins>
            <plugin>
                <artifactId>maven-archetype-plugin</artifactId>
                <version>2.0-alpha-3-SNAPSHOT</version>
                <extensions>true</extensions>
            </plugin>
        </plugins>
    </build>

</project>
